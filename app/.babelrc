{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": [
              ">0.03%"
            ]
          }
        }
      }
    ]
  ],
  "plugins": [
    "inline-react-svg",
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false,
        "pure": true
      }
    ]
  ]
}
